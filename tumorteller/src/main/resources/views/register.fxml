<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import com.tumorteller.controller.*?>

<AnchorPane id="AnchorPane" prefHeight="398.0"
	prefWidth="548.0" style="-fx-background-color: white;"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.tumorteller.controller.RegisterController">
	<children>

		<Button fx:id="btnRegister" layoutX="350.0" layoutY="268.0"
			onAction="#handleButtonAction" prefHeight="25.0" prefWidth="85.0"
			style="-fx-background-color: #362159; -fx-background-radius: 90;"
			text="Register" textFill="WHITE">
			<font>
				<Font name="System Bold" size="12.0" />
			</font>
		</Button>
		<TextField fx:id="txtUsername" layoutX="321.0"
			layoutY="145.0" prefHeight="25.0" prefWidth="177.0"
			promptText="Username"
			style="-fx-background-color: -fx-text-box-border, white; -fx-background-radius: 0; -fx-background-insets: 0, 0 0 1 0;" />
		<PasswordField fx:id="txtPassword" layoutX="321.0"
			layoutY="203.0" prefHeight="25.0" prefWidth="177.0"
			promptText="Password"
			style="-fx-background-color: -fx-text-box-border, white; -fx-background-radius: 0; -fx-background-insets: 0, 0 0 1 0;" />
		<ImageView fitHeight="25.0" fitWidth="28.0" layoutX="274.0"
			layoutY="145.0" pickOnBounds="true" preserveRatio="true">
			<image>
				<Image url="@../icons/user.JPG" />
			</image>
		</ImageView>
		<ImageView fitHeight="27.0" fitWidth="34.0" layoutX="278.0"
			layoutY="203.0" pickOnBounds="true" preserveRatio="true">
			<image>
				<Image url="@../icons/password.JPG" />
			</image>
		</ImageView>

		<AnchorPane prefHeight="406.0" prefWidth="222.0"
			style="-fx-background-color: #362159;">
			<children>
				<Label layoutX="44.0" layoutY="83.0" text="Tumor Teller"
					textFill="WHITE">
					<font>
						<Font size="24.0" />
					</font>
				</Label>
			</children>
		</AnchorPane>
	</children>
</AnchorPane>
