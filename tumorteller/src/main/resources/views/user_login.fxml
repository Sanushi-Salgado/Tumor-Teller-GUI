<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import com.tumorteller.controller.*?>

<AnchorPane id="AnchorPane" prefHeight="398.0"
	prefWidth="548.0" style="-fx-background-color: white;"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.tumorteller.controller.UserLoginController">
	<children>
		<Button fx:id="btnLogin" layoutX="350.0" layoutY="256.0"
			onAction="#handleLoginButton" prefHeight="25.0" prefWidth="85.0"
			style="-fx-background-color: #362159; -fx-background-radius: 90;"
			text="Login" textFill="WHITE">
			<font>
				<Font name="System Bold" size="12.0" />
			</font>
		</Button>
		<TextField fx:id="txtUsername" layoutX="321.0"
			layoutY="133.0" prefHeight="25.0" prefWidth="177.0"
			promptText="Username"
			style="-fx-background-color: -fx-text-box-border, white; -fx-background-radius: 0; -fx-background-insets: 0, 0 0 1 0;" />
		<PasswordField fx:id="txtPassword" layoutX="321.0"
			layoutY="191.0" prefHeight="25.0" prefWidth="177.0"
			promptText="Password"
			style="-fx-background-color: -fx-text-box-border, white; -fx-background-radius: 0; -fx-background-insets: 0, 0 0 1 0;" />
		<ImageView fitHeight="25.0" fitWidth="28.0" layoutX="274.0"
			layoutY="133.0" pickOnBounds="true" preserveRatio="true">
			<image>
				<Image url="@../icons/user.JPG" />
			</image>
		</ImageView>
		<ImageView fitHeight="27.0" fitWidth="34.0" layoutX="278.0"
			layoutY="191.0" pickOnBounds="true" preserveRatio="true">
			<image>
				<Image url="@../icons/password.JPG" />
			</image>
		</ImageView>

		<AnchorPane prefHeight="405.0" prefWidth="225.0"
			style="-fx-background-color: #362159;">
			<children>
				<Label layoutX="46.0" layoutY="203.0" text="Tumor Teller"
					textFill="WHITE">
					<font>
						<Font size="24.0" />
					</font>
				</Label>
				<ImageView fitHeight="83.0" fitWidth="85.0" layoutX="69.0"
					layoutY="120.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../images/logo.jpg" />
					</image>
				</ImageView>
			</children>
		</AnchorPane>
		<Label fx:id="lblUser" layoutX="354.0" layoutY="78.0"
			text="User Login">
			<font>
				<Font name="System Bold" size="14.0" />
			</font>
		</Label>
		<Button fx:id="btnBack" layoutX="350.0" layoutY="294.0"
			onAction="#handleBackButton" prefHeight="25.0" prefWidth="85.0"
			style="-fx-background-color: #362159; -fx-background-radius: 90;"
			text="Back" textFill="WHITE">
			<font>
				<Font name="System Bold" size="12.0" />
			</font>
		</Button>
	</children>
</AnchorPane>
